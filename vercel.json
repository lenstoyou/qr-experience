{
  "version": 2,
  "builds": [
    { "src": "web/index.js", "use": "@vercel/node" },
    {
      "src": "web/frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "web/index.js" },
    { "src": "/auth/(.*)", "dest": "web/index.js" },
    { "src": "/webhook/(.*)", "dest": "web/index.js" },
    { "handle": "filesystem" },
    { "src": "(.*)", "dest": "web/index.js" }
  ]
}